package dejavu.graphics.text;

import org.lwjgl.util.vector.Vector2f;
import org.lwjgl.util.vector.Vector3f;

import dejavu.engine.graphics.Texture;
import dejavu.game.graphics.Drawable;

public class Character extends Drawable{

	protected Character(Vector2f position, float fontSize, Texture texture) {
		super(position, 0f, new Vector2f(fontSize * 1f, fontSize * 2f), texture);
	}

}
